
@foreach (var item in Model.ListBuildingControls)
{
    <tr>

        <td class="center"> <a class="btn btn-primary modal-link" href="@Url.Action("ControlsDetails", "BuildingControls", new { @Id = item.Id })"> @item.TransactNo  </a> </td>
        <td class="center">@item.ServiceType</td>

        @{
            var User = DMeServices.Models.Common.UserCom.UserByCivilID((long)item.ConsultantCivilId);

            if (User != null)
            {
                <td class="center">
                    @User.CompanyName
                </td>
            }
            else
            {
                <td class="center">
                    لايوجد
                </td>
            }

        }
        <td class="center">@item.OwnerName</td>
        <td class="center">@item.DistrictName</td>
        <td class="center">@item.RequestDate.ToShortDateString()</td>
        <td class="center" style="color:#2f9ce5;">@DMeServices.Models.Common.LookupsTypeCom.LookupByID((int)item.WorkflowStatus).LookupNameAr</td>
        <td class="center"> @item.OwnerCivilId</td>
        @if (Model.oEmployeeInfo.IsEngineerHead)
        {


            if (item.DmEngineerNo == null)
            {
                <td class="center"> @Html.DisplayName(DMeServices.Models.Resources.DisplayName_Ar.NotFound)</td>
            }
            else
            {
                <td class="center">@DMeServices.Models.Common.EmployeeCom.EmployeeByID(item.DmEngineerNo.ToString()).NAME </td>
            }

        }
    </tr>
}


